pipeline {
    agent none
    stages {
        stage('Compile') {
            agent any
            steps {
		        sh "mvn compile"
           } 
        }
        stage ('UnitTest') {
            agent any
            steps {
		        sh "mvn test"
            }
        }
        stage ('Package') {
            agent {
                label 'linux_slave'
            }
            steps {
		    sh "mvn package"
            }
        }
    }
}
