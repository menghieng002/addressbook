pipeline {
    agent any
    stages {
        stage('Compile') {
           steps {
               echo "Compile the code"
              
           } 
        }
        stage ('UnitTest') {
            steps {
                echo "Test the code"
                
            }
        }
        stage ('Package') {
            steps {
                echo "Package the code"
                
            }
        }
    }
}
